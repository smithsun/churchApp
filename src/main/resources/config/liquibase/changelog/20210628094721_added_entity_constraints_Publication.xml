        <addForeignKeyConstraint baseColumnNames="public_notice_id"
                                 baseTableName="publication"
                                 constraintName="fk_publication__public_notice_id"
                                 referencedColumnNames="id"
                                 referencedTableName="public_notice"/>
    </changeSet>
</databaseChangeLog>
